!!-- urxvt
URxvt*.scrollBar: false
URxvt*.font: -*-clean-medium-r-normal-*-12-*-*-*-*-*-*-*
URxvt*.intensityStyles: false
URxvt*.internalBorder: 4
URxvt*.highlightColor: #303030

!!-- urxvt perl select
URxvt.iso14755: false
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher:  /home/vixus/bin/www
URxvt.url-select.underline: true
URxvt.keysym.M-w: perl:url-select:select_next
URxvt.keysym.M-a: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.keysym.C-A-V: perl:clipboard:paste escaped

