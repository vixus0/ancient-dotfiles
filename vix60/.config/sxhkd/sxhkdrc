super + shift + r
  pkill -SIGUSR1 sxhkd

super + {8,9,0}
  chvol {mute,down,up}

super + Return 
  termite -e fish

super + Escape
  dmlaunch

super + F5
  scrot -e 'mv $f ~/shots/'

super + button3
  onboard 
